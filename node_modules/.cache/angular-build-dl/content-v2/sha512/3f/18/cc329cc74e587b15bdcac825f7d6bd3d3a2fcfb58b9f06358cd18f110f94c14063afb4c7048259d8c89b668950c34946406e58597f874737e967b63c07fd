}),c=this.transformValue(o,TO),u=this.transformValue(r,kO,{firstSign:" / "}),h=this.getElectronicCityValue(n,l),d=`${this.addAuthorsToRefer(t,`${a}${c}`,this.selectedSigns,this.isMoreThanFourAuthors)}${u}.${h} ${s}.`;return{reference:d,transliteration:this.getTransliteration(d)}}getRemoteElectronicItemReference(e){const{authors:t="",city:n,date:r,elecResource:i,resource:s,url:o,year:l}=e,a=this.translateToUppercase(i),c=this.transformValue(s),u=this.editerDate(r),h=this.getElectronicCityValue(n,l),d=`${this.addAuthorsToRefer(t,a,this.selectedSigns,this.isMoreThanFourAuthors)} // ${c}.${h} URL: ${o} (${this.selectedSigns.date} ${u}).`;return{reference:d,transliteration:this.getTransliteration(d)}}getWebSiteItemReference(e){const{pageOfWebsite:t="",city:n,date:r,nameOfWebsite:i,url:s,year:o}=e,l=this.transformValue(i,kO,{lastSign:this.selectedSigns.web}),a=this.transformValue(t,kO,{lastSign:" // "}),c=this.editerDate(r),u=`${a}${l}.${this.getElectronicCityValue(n,o)} URL: ${s} (${this.selectedSigns.date} ${c}).`;return{reference:u,transliteration:this.getTransliteration(u)}}getInternetPortalItemReference(e){const{city:t,date:n,nameOfWebsite:r,url:i,year:s}=e,o=this.transformValue(r,kO,{lastSign:this.selectedSigns.portal}),l=this.editerDate(n),a=`${o}.${this.getElectronicCityValue(t,s)} URL: ${i} (${this.selectedSigns.date} ${l}).`;return{reference:a,transliteration:this.getTransliteration(a)}}createPaperReference(e,t){switch(t){case"articleFromMagazine":case"articleFromVolume":case"articleFromScienceWorks":return this.getPaperItemReference(e);case"articleFromBook":return this.getPaperOfBookItemReference(e);case"articleFromNewspapper":return this.getNewspaperItemReference(e)}}createElectronicReference(e,t){switch(t){case"localElectronicResourse":return this.getLocalElectronicItemReference(e);case"remoteElectronicResourse":return this.getRemoteElectronicItemReference(e);case EO:case"pageFromwebSiteResourse":return this.getWebSiteItemReference(e);case"internetPortal":ret