is.getRemoteElectronicItemBibliography(values);case WEBSITE_ITEM:case\"pageFromwebSiteResourse\":return this.getWebSiteItemBibliography(values);case\"internetPortal\":return this.getInternetPortalItemBibliography(values)}}createBibliographyItem(option,paperOption,electronicOption,cartographicOption,rulesOption,values,selectedSigns,isMoreThanFourAuthors){const isAbsctractThesis=\"abstactOfThesis\"===option;switch(this.isMoreThanFourAuthors=isMoreThanFourAuthors,this.selectedSigns=selectedSigns,option){case\"bookItem\":case\"seriecResourseItem\":case\"multiVolumeBooksItem\":return this.getBookItemBibliography(values);case\"partOfBookItem\":return this.getBookPartItemBibliography(values);case\"scienceWorksItem\":return this.getScienceWorkBibliography(values);case\"legisDocumentItem\":return this.getLegisyBibliography(values);case\"standartDescriptionItem\":return this.getStandartBibliography(values);case\"tеhesis\":case\"abstactOfThesis\":return this.getThesisItemBibliography(values,isAbsctractThesis);case\"yearBooksResource\":return this.getYearBooksBibliography(values);case\"catalogsResource\":return this.getCatalogsBibliography(values);case\"typicalProjectResource\":return this.getTypicalProjectBibliography(values);case\"priceListResource\":return this.getPriceListBibliography(values);case\"postcardResource\":return this.getPostcardBibliography(values);case\"interviewResource\":return this.getInterviewBibliography(values);case PAPER_ITEM:return this.createPaperBibliography(values,paperOption);case ELECTRONIC_ITEM:return this.createElectronicBibliography(values,electronicOption);case\"rulesAndRegulationsResource\":return this.createRulesAndRegulationsBibliography(values,rulesOption);case\"cartographicEditionsResource\":return this.createCartographicBibliography(values,cartographicOption)}}}return BibliographyService.ɵprov=ɵɵdefineInjectable({factory:function(){return new BibliographyService},token:BibliographyService,providedIn:\"root\"}),BibliographyService})();cons