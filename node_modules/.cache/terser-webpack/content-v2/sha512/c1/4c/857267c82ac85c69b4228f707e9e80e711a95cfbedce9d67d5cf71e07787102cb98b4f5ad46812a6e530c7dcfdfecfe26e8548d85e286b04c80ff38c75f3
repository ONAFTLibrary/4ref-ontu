OfPages:\" P. \",series:\", No. \",bookEditor:\" / editor. by \",resource:\" [Electronic resource]: \",date:\"viewed on: \",web:\": [Website]\",portal:\" : [Internet portal]\",volume:\" Vol. \",volumeNum:\"#\",topic:\"Topic \",month:{0:\"January\",1:\"February\",2:\"March\",3:\"April\",4:\"May\",5:\"June\",6:\"July\",7:\"Август\",8:\"September\",9:\"October\",10:\"November\",11:\"December\"},number:\"№ \"},PAPER_ITEM=\"paperResource\",ELECTRONIC_ITEM=\"electronicResource\",WEBSITE_ITEM=\"webSiteResourse\",UPPERCASE=\"uppercase\",LOWERCASE=\"lowercase\";class base_component_BaseComponent{constructor(){}getTransliteration(value){let currentValue,transLetter,result=\"\",curentSymbol=\"\";for(let i=0,li=value.length;i<li;i++)currentValue=value[i],transLetter=TRANSLATIONS_LETTERS[currentValue],transLetter?(result+=transLetter,curentSymbol=transLetter):(result+=currentValue,curentSymbol=currentValue);return result}getAuthors(authors,reverse){let currentAuthor,currentName,currentLastName,currentPatronymic,authorStr=[];for(let i=0,li=authors.length;i<li;i++){const{name:name,lastName:lastName,patronymic:patronymic}=authors[i];currentLastName=this.formateStr(lastName),currentName=this.formateFirstLetter(name),currentPatronymic=patronymic?this.formateFirstLetter(patronymic):\"\",currentAuthor=reverse?`${currentName} ${currentPatronymic} ${currentLastName}`:`${currentLastName} ${currentName} ${currentPatronymic}`,authorStr.push(currentAuthor)}return authorStr.join(\", \")}addAuthorsToRefer(authors,mainPart,selectedSigns,isMoreThanFourAuthors,bibliography){if(!authors)return\"\"+mainPart;const authorsCount=bibliography?authors.slice(0,1):authors,authorStr=this.getAuthors(authorsCount),authorReverseStr=this.getAuthors(authors,!0);return isMoreThanFourAuthors?`${mainPart} / ${bibliography?authorReverseStr:authorStr}${bibliography?selectedSigns.biblAuthor:selectedSigns.authors}`:`${authorStr} ${mainPart}${bibliography?\" / \"+authorReverseStr:\"\"}`}formatOfName(value,fullName){ret