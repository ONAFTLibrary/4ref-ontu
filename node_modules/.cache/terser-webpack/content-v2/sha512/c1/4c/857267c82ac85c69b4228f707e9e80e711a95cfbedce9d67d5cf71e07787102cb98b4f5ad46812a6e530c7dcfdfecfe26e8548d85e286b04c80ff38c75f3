ctedSigns.biblSafe}),dateOfProtectStr=this.editerDate(dateOfProtect),dateApproveValue=this.editerDate(dateApprove),dateApproveStr=this.transformValue(dateApproveValue,LOWERCASE,{firstSign:this.selectedSigns.acceptInfo}),notatesStr=this.transformValue(notates,UPPERCASE,{firstSign:this.selectedSigns.thesisNotates}),reference=`${author} ${workNameStr}${degreeStr} : ${cipherStr}${dateOfProtectStr}${dateApproveStr} / ${fullName}. - ${this.translateToUppercase(city)}${this.getExtensionValue(publisher,UPPERCASE)}${this.getYearValue(year)}. - ${this.getPagesValue(pages,{lastSign:this.selectedSigns.pages})}.${notatesStr}`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getPaperItemBibliography(values){const{authors:authors=\"\",copyrigth:copyrigth,magazineName:magazineName,pages:pages,paperName:paperName,partOfMagazine:partOfMagazine,volumeOfMagazine:volumeOfMagazine,year:year,city:city,notates:notates}=values,{series:series,number:number,volume:volume,scaleOfPages:scaleOfPages}=this.selectedSigns,magazineNameStr=magazineName?\" // \"+magazineName:\"\",paperNameStr=this.translateToUppercase(paperName),partOfMagazineStr=this.transformValue(partOfMagazine,LOWERCASE,{firstSign:volumeOfMagazine?series:number}),volumeOfMagazineStr=this.transformValue(volumeOfMagazine,LOWERCASE,{firstSign:volume}),pagesStr=this.getPagesValue(pages,{firstSign:scaleOfPages}),copyrigthStr=this.transformValue(copyrigth,UPPERCASE,{firstSign:\" / \"}),yearStr=this.getYearValue(year),cityStr=this.translateToUppercase(city),notatesStr=this.transformValue(notates,UPPERCASE,{firstSign:\" - \",lastSign:\".\"}),reference=`${this.addAuthorsToRefer(authors,paperNameStr,this.selectedSigns,this.isMoreThanFourAuthors,!0)}${magazineNameStr}${copyrigthStr}. - ${cityStr}${city?yearStr:year}. - ${volumeOfMagazineStr}${partOfMagazineStr}. - ${pagesStr}.${notatesStr}`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getPaperOfBookItemBibliography(values){const{authors:authors=\"\",authorsOfBook:authorsOfBook,articleEditor:articleEditor,typeOfBook:typeOfBook,bookName:bookName,pages:pages,paperName:paperName,city:city,year:year}=values,{bookEditor:bookEditor,biblBookEditor:biblBookEditor,scaleOfPages:scaleOfPages}=this.selectedSigns,bookNameStr=this.translateToUppercase(bookName),paperNameStr=this.translateToUppercase(paperName),authorsOfBookStr=this.transformValue(authorsOfBook,UPPERCASE,{firstSign:\" / \"}),articleEditorStr=this.getEditorValue(articleEditor,{firstSign:Boolean(authorsOfBook)?biblBookEditor:bookEditor}),typeOfBookStr=this.getExtensionValue(typeOfBook,LOWERCASE),cityStr=this.translateToUppercase(city),pagesStr=this.getPagesValue(pages,{firstSign:scaleOfPages}),yearStr=this.getYearValue(year),reference=`${this.addAuthorsToRefer(authors,paperNameStr,this.selectedSigns,this.isMoreThanFourAuthors,!0)} // ${bookNameStr}${typeOfBookStr}${authorsOfBookStr}${articleEditorStr}. - ${cityStr}${yearStr}. - ${pagesStr}.`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getNewspaperItemBibliography(values){const{authors:authors=\"\",bookName:bookName,pages:pages,paperName:paperName,publishDate:publishDate,publishNum:publishNum,year:year}=values,convertDate=new Date(publishDate),paperNameStr=this.translateToUppercase(paperName),bookNameStr=this.translateToUppercase(bookName),yearStr=this.transformValue(year,LOWERCASE,{lastSign:\". \"}),pagesStr=this.getPagesValue(pages,{firstSign:this.selectedSigns.scaleOfPages}),volumeStr=this.transformValue(publishNum,LOWERCASE,{firstSign:this.selectedSigns.volumeNum}),publishDateStr=`${convertDate.getDate()} ${this.selectedSigns.month[convertDate.getMonth()]}`,reference=`${this.addAuthorsToRefer(authors,paperNameStr,this.selectedSigns,this.isMoreThanFourAuthors,!0)} // ${bookNameStr}. - ${yearStr}${publishDateStr} (${volumeStr}). - ${pagesStr}.`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getLocalElectronicItemBibliography(values){const{authors:authors=\"\",notates:notates,created:created,city:city,copyrigth:copyrigth,elecResource:elecResource,typeOfAccess:typeOfAccess,typeOfResource:typeOfResource,year:year}=values,elecResourceStr=this.transformValue(elecResource,UPPERCASE,{lastSign:this.selectedSigns.resource}),typeOfResourceStr=this.transformValue(typeOfResource,LOWERCASE),notatesStr=this.transformValue(notates,UPPERCASE,{firstSign:\" - \",lastSign:\".\"}),cityStr=this.transformValue(city,UPPERCASE,{firstSign:\". - \",lastSign:copyrigth?\": \":\"\"}),copyrigthStr=this.transformValue(copyrigth,UPPERCASE,{firstSign:city?\"\":\". - \"}),yearStr=city||copyrigth?this.getYearValue(year):this.transformValue(copyrigth,UPPERCASE,{firstSign:\". - \"}),createdStr=this.transformValue(created,UPPERCASE,{firstSign:this.selectedSigns.editors}),reference=`${this.addAuthorsToRefer(authors,`${elecResourceStr}${typeOfResourceStr}`,this.selectedSigns,this.isMoreThanFourAuthors,!0)}${createdStr}${cityStr}${copyrigthStr}${yearStr}. - ${typeOfAccess}.${notatesStr}`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getRemoteElectronicItemBibliography(values){const{authors:authors=\"\",city:city,date:date,elecResource:elecResource,resource:resource,url:url,year:year,notates:notates,created:created}=values,elecResourceStr=this.translateToUppercase(elecResource),resourceStr=this.transformValue(resource),dateStr=this.editerDate(date),appendPartStr=this.getElectronicCityValue(city,year,!0),notatesStr=this.transformValue(notates,UPPERCASE,{lastSign:\". - \"}),createdStr=this.transformValue(created,UPPERCASE,{firstSign:this.selectedSigns.editors}),reference=`${this.addAuthorsToRefer(authors,elecResourceStr,this.selectedSigns,this.isMoreThanFourAuthors,!0)}${createdStr} // ${resourceStr}. - ${appendPartStr}${notatesStr}URL: ${url} (${this.selectedSigns.date} ${dateStr}).`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getWebSiteItemBibliography(values){const{pageOfWebsite:pageOfWebsite=\"\",city:city,date:date,nameOfWebsite:nameOfWebsite,url:url,year:year}=values,nameOfWebsiteStr=this.transformValue(nameOfWebsite,UPPERCASE,{lastSign:this.selectedSigns.webBibl}),pageOfWebsiteStr=this.transformValue(pageOfWebsite,UPPERCASE,{lastSign:\" // \"}),dateStr=this.editerDate(date),reference=`${pageOfWebsiteStr}${nameOfWebsiteStr}. - ${this.getElectronicCityValue(city,year,!0)}URL: ${url} (${this.selectedSigns.date} ${dateStr}).`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getInternetPortalItemBibliography(values){const{city:city,date:date,nameOfWebsite:nameOfWebsite,url:url,year:year}=values,nameOfWebsiteStr=this.transformValue(nameOfWebsite,UPPERCASE,{lastSign:this.selectedSigns.portal}),dateStr=this.editerDate(date),reference=`${nameOfWebsiteStr}. - ${this.getElectronicCityValue(city,year,!0)}URL: ${url} (${this.selectedSigns.date} ${dateStr}).`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getConstructionRulesBibliography(values){const{cypher:cypher,nameOfStandart:nameOfStandart,typeOfStandart:typeOfStandart,city:city,pages:pages,date:date,year:year}=values,cypherStr=this.translateToUppercase(cypher),typeOfStandartStr=this.translateToUppercase(typeOfStandart),nameOfStandartStr=this.translateToUppercase(nameOfStandart),dateStr=this.editerDate(date),yearStr=this.getYearValue(year),cityStr=this.translateToUppercase(city),pagesStr=this.getPagesValue(pages,{lastSign:this.selectedSigns.pages}),reference=`${cypherStr}. ${typeOfStandartStr}. ${nameOfStandartStr}. - [${this.selectedSigns.actual}${dateStr}]. - ${cityStr}${yearStr}. - ${pagesStr}.`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getSanitaryRulesBibliography(values){const{cypher:cypher,nameOfStandart:nameOfStandart,typeOfStandart:typeOfStandart,city:city,pages:pages,approved:approved,date:date,year:year}=values,cypherStr=this.translateToUppercase(cypher),typeOfStanda