cOption,values,selectedSigns,isMoreThanFourAuthors){const isAbsctractThesis=\"abstactOfThesis\"===option;switch(this.isMoreThanFourAuthors=isMoreThanFourAuthors,this.selectedSigns=selectedSigns,option){case\"bookItem\":case\"multiVolumeBooksItem\":return this.getBookItemReference(values);case\"partOfBookItem\":return this.getPartOfBookReference(values);case\"scienceWorksItem\":return this.getScienceWork(values);case\"seriecResourseItem\":return this.getSeriesResourseItemReference(values);case\"legisDocumentItem\":return this.getLigestiveItemReferences(values);case\"standartDescriptionItem\":return this.getStandartItemReference(values);case\"tеhesis\":case\"abstactOfThesis\":return this.getThesisItemReference(values,isAbsctractThesis);case PAPER_ITEM:return this.createPaperReference(values,paperOption);case ELECTRONIC_ITEM:return this.createElectronicReference(values,electronicOption)}}}return ReferencesService.ɵprov=ɵɵdefineInjectable({factory:function(){return new ReferencesService},token:ReferencesService,providedIn:\"root\"}),ReferencesService})();class WebStorageService{constructor(storage){this.storage=storage}get(key){try{return JSON.parse(this.storage.getItem(key))}catch(error){return null}}set(key,value){this.storage.setItem(key,JSON.stringify(value))}remove(key){this.storage.removeItem(key)}}function isStorageAvailable(storage){if(!storage)return!1;try{const now=Date.now(),testItemKey=\"storage-test-entry-\"+now,testItemValue=\"storage-test-value-\"+now;storage.setItem(testItemKey,testItemValue);const retrievedItemValue=storage.getItem(testItemKey);return storage.removeItem(testItemKey),retrievedItemValue===testItemValue}catch(error){return!1}}class InMemoryStorageService{constructor(){this.storage=new Map}get(key){return this.storage.has(key)?this.storage.get(key):null}set(key,value){this.storage.set(key,value)}remove(key){this.storage.delete(key)}}const SESSION_STORAGE=new InjectionToken(\"SESSION_STORAGE\"),LOCAL_STORAGE=new InjectionToken(\"LOCAL_STORAGE\");function sessionStorageFactory(){return isStorageAvailable(sessionStorage)?new WebStorageService(sessionStorage):new InMemoryStorageService}function localStorageFactory(){return isStorageAvailable(localStorage)?new WebStorageService(localStorage):new InMemoryStorageService}class StorageServiceModule{}StorageServiceModule.decorators=[{type:NgModule,args:[{providers:[{provide:SESSION_STORAGE,useFactory:sessionStorageFactory},{provide:LOCAL_STORAGE,useFactory:localStorageFactory}]}]}],StorageServiceModule.ctorParameters=()=>[];let local_storage_service_LocalStorageService=(()=>{class LocalStorageService{constructor(storage){this.storage=storage}storeReferncesOnLocalStorage(referncesItems){this.storage.set(\"referncesItems\",referncesItems)}getReferncesValuesFromLocalStorage(){return this.storage.get(\"referncesItems\")}storeBibliographyOnLocalStorage(bibliographyItems){this.storage.set(\"bibliographyItems\",bibliographyItems)}getBibliographyValuesFromLocalStorage(){return this.storage.get(\"bibliographyItems\")}}return LocalStorageService.ɵprov=ɵɵdefineInjectable({factory:function(){return new LocalStorageService(ɵɵinject(LOCAL_STORAGE))},token:LocalStorageService,providedIn:\"root\"}),LocalStorageService})();var build=__webpack_require__(\"Sfth\"),FileSaver_min=__webpack_require__(\"Iab2\");let save_to_docx_service_SaveToDocxService=(()=>{class SaveToDocxService{constructor(){}saveListItems(values,isPaper){const doc=new build.Document,docxName=isPaper?\"References.docx\":\"List.docx\";doc.addSection({children:[new build.Paragraph({children:[new build.TextRun({text:\"Список використаних джерел\",bold:!0})],alignment:build.AlignmentType.CENTER}),...values.map(({reference:reference},i)=>this.generateList(reference,i,isPaper)),new build.Paragraph({children:[new build.TextRun({text:\"References\",bold:!0})],alignment:build.AlignmentType.CENTER}),...values.map(({transliteration:transliteration},i)=>this.generateList(transliteration,i,isPaper))]}),build.Packer.toBlob(do