,pages:pages,paperName:paperName,publishDate:publishDate,publishNum:publishNum,year:year}=values,convertDate=new Date(publishDate),paperNameStr=this.translateToUppercase(paperName),bookNameStr=this.translateToUppercase(bookName),yearStr=this.transformValue(year,LOWERCASE,{lastSign:\". \"}),pagesStr=this.getPagesValue(pages,{firstSign:this.selectedSigns.scaleOfPages}),volumeStr=this.transformValue(publishNum,LOWERCASE,{firstSign:this.selectedSigns.volumeNum}),publishDateStr=`${convertDate.getDate()} ${this.selectedSigns.month[convertDate.getMonth()]}`,reference=`${this.addAuthorsToRefer(authors,paperNameStr,this.selectedSigns,this.isMoreThanFourAuthors)} // ${bookNameStr}. ${yearStr}${publishDateStr} (${volumeStr}). ${pagesStr}.`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getLocalElectronicItemReference(values){const{authors:authors=\"\",city:city,copyrigth:copyrigth,elecResource:elecResource,typeOfAccess:typeOfAccess,typeOfResource:typeOfResource,year:year}=values,elecResourceStr=this.transformValue(elecResource,UPPERCASE,{lastSign:this.selectedSigns.resource}),typeOfResourceStr=this.transformValue(typeOfResource,LOWERCASE),copyrigthStr=this.transformValue(copyrigth,UPPERCASE,{firstSign:\" / \"}),appendPart=this.getElectronicCityValue(city,year),reference=`${this.addAuthorsToRefer(authors,`${elecResourceStr}${typeOfResourceStr}`,this.selectedSigns,this.isMoreThanFourAuthors)}${copyrigthStr}.${appendPart} ${typeOfAccess}.`;return{reference:reference,transliteration:this.getTransliteration(reference)}}getRemoteElectronicItemReference(values){const{authors:authors=\"\",city:city,date:date,elecResource:elecResource,resource:resource,url:url,year:year}=values,elecResourceStr=this.translateToUppercase(elecResource),resourceStr=this.transformValue(resource),dateStr=this.editerDate(date),appendPart=this.getElectronicCityValue(city,year),reference=`${this.addAuthorsToRefer(authors,elecResourceStr,this.selectedSigns,this.isMoreThanFourAutho